 /* Tree View */
 QTreeView {
 	show-decoration-selected: 0;
 }
 TreeView {
 	background: #111;
 	border: 1px solid #111;
 	color: #bbb;
 }
 QTreeView::item {
 	height: 16px;
 	border: 0px;
 }
 QTreeView::item:hover {
 	font-weight: bold;
 	color: #fff;
 }
 QTreeView::item:selected:active {
 	color: #fff;
 	font-weight: bold;
 }
 QTreeView::item:selected:!active {
 	color: #fff;
 }













 /* QTreeView::branch:selected:!active -> wird vom :active �bernommen */
 QTreeView::branch:selected {
 	background: none;
 }
 QTreeView::branch:has-siblings:!adjoins-item {
 	border-image: url(styles:aod/images/sibling_branch.png) 0;
 }
 QTreeView::branch:has-siblings:adjoins-item {
 	border-image: url(styles:aod/images/sibling_branch.png) 0;
 }
 QTreeView::branch:!has-children:!has-siblings:adjoins-item {
 	border-image: url(styles:aod/images/sibling_branch.png) 0;
 }
 QTreeView::branch:has-children:!has-siblings:closed,
 QTreeView::branch:closed:has-children:has-siblings {
 	border-image: none;
 	image: url(styles:aod/images/open_arrow.png);
 }
 QTreeView::branch:open:has-children:!has-siblings,
 QTreeView::branch:open:has-children:has-siblings  {
 	border-image: none;
 	image: url(styles:aod/images/close_arrow.png);
 }
 QTreeView#eventsTreeView::indicator:checked, QTreeView#permissionsTreeView::indicator:checked, QTreeWidget#appsTreeWidget::indicator:checked, QListWidget#pluginsListWidget::indicator:checked, QListView#ServerGroupsListView::indicator:checked, QTreeWidget#clientsTreeWidget::indicator:checked, QGroupBox#advancedGroupBox::indicator:checked {
 	background-color: #c1daff;
 	border: 1px solid #000;
 	border-radius: 1px;

/* ScrollBar vertical */
QScrollBar:vertical {
	border: 1px solid #0a0a0a;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #181818, stop: 1 #181818);
	width: 15px;
	margin: 17px 0 17px 0;
}
QScrollBar::handle:vertical {
	border: 1px solid #454545;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #454545, stop: 1 #454545);
	min-height: 20px;
	border-radius: 5px;
}
QScrollBar::add-line:vertical  {
	border: 1px solid #222222;
	background: #444444;
	height: 15px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
	border-radius: 2px;
}
QScrollBar::sub-line:vertical {
	border: 1px solid #222222;
	background: #444444;
	height: 15px;
	subcontrol-position: top;
	subcontrol-origin: margin;
	border-radius: 2px;
}
QScrollBar::up-arrow:vertical {
	background: url("styles/KGN/up_arrow.png");
	background-repeat: none;
	left: 3px;
	top: 5px;
}
QScrollBar::down-arrow:vertical {
	background: url("styles/KGN/down_arrow.png");
	background-repeat: none;
	left: 3px;
	top: 5px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}


/* ScrollBar horizontal */
QScrollBar:horizontal {
	border: 1px solid #0a0a0a;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #181818, stop: 1 #181818);
	height: 15px;
	margin: 0px 17px 0 17px;
}
QScrollBar::handle:horizontal {
	border: 1px solid #454545;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #454545, stop: 1 #454545);
	min-width: 20px;
	border-radius: 5px;
}
QScrollBar::add-line:horizontal  {
	border: 1px solid #222222;
	background: #444444;
	width: 15px;
	subcontrol-position: right;
	subcontrol-origin: margin;
	border-radius: 2px;
}
QScrollBar::sub-line:horizontal {
	border: 1px solid #222222;
	background: #444444;
	width: 15px;
	subcontrol-position: left;
	subcontrol-origin: margin;
	border-radius: 2px;
}
QScrollBar::left-arrow:horizontal {
	background: url("styles/KGN/left_arrow.png");
	background-repeat: none;
	left: 5px;
	top: 3px;
}
QScrollBar::right-arrow:horizontal {
	background: url("styles/KGN/right_arrow.png");
	background-repeat: none;
	left: 5px;
	top: 3px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}

QSizeGrip {
	background-color: transparent;
	image: url("styles/KGN/sizegrip.png");
	width: 16px;
	height: 16px;

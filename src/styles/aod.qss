 /* PushButton */
 QPushButton {
 	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop: 0 #6a96b9, stop: 1 #c2d7e1);
 	border: 1px solid #0a057b;
 	border-radius: 3px;
 	padding-left: 14px; padding-right: 14px;
 	padding-top: 3px; padding-bottom: 3px;
 }
 QPushButton:hover {
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #a5d3e8, stop:1 #ccc);
 	color: #0a057b;
 }
 QPushButton:pressed {
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #ccc);
 }
 QPushButton:disabled {
 	border: 1px solid #0a057b;
 	border-radius: 3px;
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #ccc, stop: 1 #ddd);
 	color: #7b7b7b;
 }






 /* MenuBar */
 QMenuBar {
 	background-color: #444;
 	background-repeat: repeat-x;
 	spacing: 2px;
 	font-weight: bold;
 	color: #999;
 }
 QMenuBar::item {

 	background: transparent;
 	color: #fff;

 }
 QMenuBar::item:selected {
 	color: #cc0000;
 }
 QMenuBar::item:pressed {
 	background-color: #444;
 	color: #cc0000;
 }
 QMenu {
 	background-color: #333;
 	padding: 10px;
 	color: #fff;
 	margin: 1px;
 }
 QMenu::item {
 	background: transparent;
 	padding: 3px;
 	padding-left: 20px;
 	padding-right: 10px;
 }
 QMenu::item:disabled {
 	color: #222;
 }
 QMenu::item:selected {
 	background-color: #444;
 	color: #bbb;
 }
 QMenu::separator {
 	height:1px;
 	margin-bottom: 2px;
 	margin-top: 2px;
 	background: #444;
 }





 /*QMenuIndicator*/
 QMenu::indicator:checked {
 	image: url("styles:aod/images/menu_check.png");
 }
 QMenu::indicator:non-exclusive:checked {
 	image: url("styles:aod/images/menu_check.png");
 }




/* Combobox */
QComboBox {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop:1 #303030);
	border: 1px solid #222222;
	border-radius: 3px;
	color: #e5e5e5;
	min-width: 140px;
	height: 21px;
	text-align: left;
	padding-left: 5px;
	padding-right: 10px;
	selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop:1 #303030);
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop:1 #303030);
}
QComboBox:on {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop:1 #303030);
}
QComboBox::down-arrow {
	image: url('styles/KGN/down_arrow.png');
}

QComboBox#utcLocalComboBox {
	padding-left: 14px;
	padding-right: 7px;
}

QComboBox::drop-down {
	background-color: transparent;
	border-left: 1px solid #424a50;
	border-image: none;
	subcontrol-origin: padding;
	subcontrol-position: top right;
	color: #d1cfcd;
	width: 15px;
}
QComboBox QAbstractItemView {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop: 1 #181818);
	border: 1px solid #222222;
	border-radius: 3px;
	border-image: none;
	color: #e5e5e5;
}






 /* Listview */
 QListView {
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #000, stop: 1 #cc0000);
 	color:#000;
 	selection-background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #cc0000, stop:1 #000);
 	selection-color: #cc0000;
 	show-decoration-selected: 1;
 	border: 1px solid #333;
 }
 QListView::item:hover {
 	background-color: #999;
 	border: none;
 	border-radius: none;
 }
 QListView::item:selected:active {
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #505c78, stop: 1 #6f80aa);
 	color: #fff;
 	border: none;
 }
 QListView::item:selected:!active {
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #ccc);
 	color: #0a057b;
 	border: none;
 }
 QListWidget#identitiesListWidget {
 	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #c2d7e1, stop: 1 #cc0000);
 	color:#000;
 }
QListView#fileListView::item:hover {
	color: #ff9800;
	background: transparent;
}
QListView#fileListView::item:selected {
	color: #ff6600;
	background: transparent;
}






 /* Toolbar */
 QToolBar {
 	border: transparent;
 	background-color: #333;
 	spacing: 5px;
 }






/* ToolButton */
QToolButton { 
	border: 1px solid transparent;
	border-radius: none;
	background-color: none;
	color: #e5e5e5;
} 
QToolButton:pressed {
	border: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #111, stop:1 #222);
	color: #000000;
}
QToolButton:hover {
	border: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #111, stop:1 #222);
	color: #000000;
}
QToolButton:checked, QToolButton#serverQueryAdvancedButton:checked {
	border: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #111, stop:1 #222);
	color: #000000;
}
QToolButton::menu-button {
	border: 1px solid transparent;
	border-top-right-radius: 3px;
	border-bottom-right-radius: none;
	width: 12px;
} 
QToolButton::menu-button:hover {
	border: 1px solid #000000;
}
QToolButton[popupMode="1"] {
	border: 1px solid transparent;
	padding-right: 15px;
}
QToolButton[popupMode="1"]:hover {
	border: 1px solid #000000;
	padding-right: 15px;
}
QToolButton[popupMode="1"]:checked {
	border: 1px solid #000000;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #111, stop:1 #222);
	color: #e5e5e5;
	padding-right: 15px;
}


QToolButton#closeWindowButton:hover {
	border: 1px solid #000000;
	border-radius: 3px;
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #ff9800, stop:1 #ff0066);
	color: #000000;
}








/* TabBar */

QTabBar {
	background-color: transparent;
}
QTabBar::tab {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop: 1.0 #181818);
	border: 1px solid #222222;
	color: #808080;
	min-width: 8px;
	padding: 3px;
}
QTabBar::tab:selected, QTabBar::tab:hover {
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #181818, stop: 1.0 #303030);

}
QTabBar::tab:selected {
	border-color: #222222;
	border-bottom-color: #222222;
}
QTabBar::tab:selected:top {
	border-top-color: #222222;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	margin-top: 4px;
}
QTabBar::tab:selected:bottom {
	border-bottom-color: #222222;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
}
QTabBar::tab:!selected:top {
	border-top-color: #222222;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	margin-top: 4px;
}
QTabBar::tab:!selected:bottom {
	border-bottom-color: #222222;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	margin-bottom: 4px;
}








